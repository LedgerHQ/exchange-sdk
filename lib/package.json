    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "bignumber.js": "^9.1.2",
    "eslint": "8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.6.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0"
  },
  "peerDependencies": {
    "@ledgerhq/wallet-api-client": "^1.1.0",
